<struts>
	<package name="default" extends="struts-default" >
		<!-- 用户登录 -->
		<action name="XS" class="org.action.XSAction">
			<result name="success">/xs.jsp</result>
			<result name="error">/404.html</result>
		</action>
		
		<action name="TJ" class="org.action.TJAction">
			<result name="success">/xs.jsp</result>
			<result name="error">/404.html</result>
		</action>
		
		<action name="Au" class="org.action.AuAction">
			<result name="success">/xs.jsp</result>
			<result name="error">/404.html</result>
		</action>				
		
		<action name="One" class="org.action.OneAction">
